{
    "compilerOptions": {
        "baseUrl": "",
        "rootDir": "src",
        "outDir": "lib",
        "target": "es2018",
        "module": "commonjs",
        "moduleResolution": "node",
        "noUnusedParameters": true,
        "noUnusedLocals": true,
        "declaration": true
    },
    "typedocOptions": {
        "excludeExternals": true,
        "excludePrivate": true,
        "mode": "modules",
        "name": "PandaGainz",
        "out": "docs"
    }
}
